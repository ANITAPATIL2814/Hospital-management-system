package com.demo.service;

import java.util.List;

import com.demo.entity.Appointment;

//CRUD declaration for Appointment entity
public interface AppointmentService {
	//save Appointment details in table
		Appointment saveTest(Appointment labTest);
		
		//fetch all Appointment details from table
		List<Appointment>getAllTest();
		
		//fetch lab test details based on id from table
		Appointment getTestById(Long aid);
}
